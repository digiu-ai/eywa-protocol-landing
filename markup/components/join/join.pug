include ../socials/socials.pug
include ../button/button.pug
include ../anchor/anchor.pug
mixin join(data)
    .join-proto
        +anchor('community')
        .join-proto__title Join EYWA
        .join-proto__content 
            .join-proto__section.section-proto
                .section-proto__title Community
                .section-proto__text Learn more about EYWA, chat with the team, and become the part of our growing community.
                +socials(socials.defaultList)
            .join-proto__section.section-proto._with-logo
                .section-proto__title EYWA alpha testing challenge
                .section-proto__text Take part in EYWA alpha test, complete simple tasks, earn points and get unique NFTs. 
                +button(join.joinButton)
