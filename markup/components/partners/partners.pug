mixin partners(data)
    .partners-proto
        .partners-proto__title PARTNERS
        .partners-proto__items
            each val in partners.list
                div(class="partners-proto__item" + (val.isWhitebg ? " _white-bg" : "") + ' ' + val.size)
                    img(src=val.src alt=val.alt id=val.id).partners-proto__item-icon